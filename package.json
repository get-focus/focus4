    "@types/react-dom": "16.0.5",
    "@types/react-motion": "0.0.26",
    "@types/uuid": "3.4.3",
    "i18next": "11.2.3",
    "inputmask-core": "2.2.0",
    "inversify": "4.13.0",
    "inversify-inject-decorators": "3.1.0",
