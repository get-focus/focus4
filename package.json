    "mobx": "4.2.0",
    "mobx-react": "5.0.0",
    "moment": "2.22.1",
    "numeral": "2.0.6",
    "react": "16.3.2",
    "react-css-themr": "2.1.2",
    "react-dnd": "2.6.0",
    "react-dnd-html5-backend": "2.6.0",
    "react-dom": "16.3.2",
    "react-motion": "0.5.2",
    "react-toolbox": "2.0.0-beta.12",
    "reflect-metadata": "0.1.10",
